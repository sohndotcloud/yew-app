{
  "name": "create-wasm-template",
  "version": "0.1.0",
  "description": "Boilerplate code for Web Assembly page integration with Node",
  "main": "index.node",
  "scripts": {
    "build": "cargo-cp-artifact -nc index.node -- cargo build --message-format=json-render-diagnostics",
    "build-debug": "npm run build --",
    "build-release": "npm run build -- --release",
    "install": "npm run build-release",
    "test": "cargo test"
  },
  "author": "Nils Sohn",
  "license": "MIT",
  "devDependencies": {
    "cargo-cp-artifact": "^0.1.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sohndotcloud/wasm-server.git"
  },
  "keywords": [
    "web",
    "assembly",
    "rust",
    "neon",
    "node"
  ],
  "bugs": {
    "url": "https://github.com/sohndotcloud/wasm-server/issues"
  },
  "homepage": "https://github.com/sohndotcloud/wasm-server#readme"
}
